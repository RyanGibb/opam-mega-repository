  "deb-debconf"
  "deb-init-system-helpers" {>= "1.51"}
  "deb-lsb-base"
  "deb-uuid-runtime"
  ("deb-debconf" {>= "0.5"} | "deb- debconf-2-0")
  ("deb-miniupnpd-nftables" | "deb- miniupnpd-iptables")
]
extra-source "miniupnpd_2.2.1-1_amd64.deb" {
