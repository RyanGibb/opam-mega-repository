opam-version: "2.0"
build: [
  ["sh" "-c" "sudo dpkg -i python-mocker_1.0-2_amd64.deb"]
]
remove: [
  ["sh" "-c" "sudo dpkg -r python-mocker"]
]
depends: [
  "deb-python" {< "2.8"}
  "deb-python" {>= "2.6.6-7~"}
  ("deb-python2-7" | "deb-python2-6")
]
extra-source "python-mocker_1.0-2_amd64.deb" {
  src: "http://ftp.debian.org/debian/pool/main/m/mocker/python-mocker_1.0-2_all.deb"
}
