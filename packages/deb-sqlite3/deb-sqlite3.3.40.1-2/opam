  "deb-libc6" {>= "2.34"}
  "deb-libreadline8" {>= "6.0"}
  "deb-libsqlite3-0" {= "3.40.1-2"}
  "deb-zlib1g" {>= "1-1.2.0"}
]
extra-source "sqlite3_3.40.1-2_amd64.deb" {
  src: "http://ftp.debian.org/debian/pool/main/s/sqlite3/sqlite3_3.40.1-2_amd64.deb"
}
