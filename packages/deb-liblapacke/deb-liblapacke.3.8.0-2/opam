opam-version: "2.0"
build: [
  ["sh" "-c" "sudo dpkg -i liblapacke_3.8.0-2_amd64.deb"]
]
remove: [
  ["sh" "-c" "sudo dpkg -r liblapacke"]
]
depends: [
  "deb-libc6" {>= "2.4"}
  "deb-libtmglib3"
  ("deb-libblas3" | "deb- libblas-so-3")
  ("deb-liblapack3" | "deb- liblapack-so-3")
]
extra-source "liblapacke_3.8.0-2_amd64.deb" {
  src: "http://ftp.debian.org/debian/pool/main/l/lapack/liblapacke_3.8.0-2_amd64.deb"
}
