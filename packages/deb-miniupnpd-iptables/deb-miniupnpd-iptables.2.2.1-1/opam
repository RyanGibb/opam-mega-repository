  ["sh" "-c" "sudo dpkg -r miniupnpd-iptables"]
]
depends: [
  "deb-iptables"
  "deb-libc6" {>= "2.17"}
  "deb-libip4tc2" {>= "1.8.3"}
  "deb-libip6tc2" {>= "1.8.3"}
]
conflicts: [
  "deb-miniupnpd-nftables"
]
extra-source "miniupnpd-iptables_2.2.1-1_amd64.deb" {
  src: "http://ftp.debian.org/debian/pool/main/m/miniupnpd/miniupnpd-iptables_2.2.1-1_amd64.deb"
}
