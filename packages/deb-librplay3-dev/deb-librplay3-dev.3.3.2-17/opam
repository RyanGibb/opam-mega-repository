opam-version: "2.0"
build: [
  ["sh" "-c" "sudo dpkg -i librplay3-dev_3.3.2-17_amd64.deb"]
]
remove: [
  ["sh" "-c" "sudo dpkg -r librplay3-dev"]
]
depends: [
  "deb-librplay3" {= "3.3.2-17"}
  ("deb-dpkg" {>= "1.15.4"} | "deb- install-info")
  ("deb-libc6-dev" | "deb- libc-dev")
]
extra-source "librplay3-dev_3.3.2-17_amd64.deb" {
  src: "http://ftp.debian.org/debian/pool/main/r/rplay/librplay3-dev_3.3.2-17_amd64.deb"
}
