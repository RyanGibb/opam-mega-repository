  "deb-libc6" {>= "2.2.5"}
  "deb-libgcc1" {>= "1-3.0"}
  "deb-libstdc++6" {>= "4.4.0"}
]
conflicts: [
  "deb-arpack++"
  "deb-libarpack++2"
  "deb-libarpack++2c2"
]
