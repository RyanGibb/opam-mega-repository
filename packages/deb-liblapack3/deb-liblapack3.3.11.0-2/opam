  "deb-libc6" {>= "2.29"}
  "deb-libgcc-s1" {>= "4.0"}
  "deb-libgfortran5" {>= "8"}
  ("deb-libblas3" | "deb- libblas-so-3")
]
extra-source "liblapack3_3.11.0-2_amd64.deb" {
  src: "http://ftp.debian.org/debian/pool/main/l/lapack/liblapack3_3.11.0-2_amd64.deb"
}
